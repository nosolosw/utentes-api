        <input name="passwd" type="password" class="form-control" placeholder="Contrase&ntilde;a">
        <input type="submit" name="submit" value="Entrar" class="btn btn-large btn-primary">
      </form>

    </div>

    <script src="/static/utentes-ui/lib/jquery/jquery-1.12.0.min.js"></script>
    <script src="/static/utentes-ui/lib/bootstrap/js/bootstrap.min.js"></script>

